pipeline {
    agent any
    stages {
        stage('check') {
            steps {
                sh 'hello'
            }
        }
        stage('test') {
            steps {
                echo 'Step test'
            }
        }
        stage('integration test') {
            steps {
                echo 'Step IT'
            }
        }
        stage('build') {
            steps {
                sh ''
            }
        }
        stage('run') {
            steps {
                sh ''
            }
        }
    }
}